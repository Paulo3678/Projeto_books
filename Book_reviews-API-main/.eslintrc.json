{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:jest/recommended",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },"plugins": [
        "jest"
    ],
    "rules": {
        "no-console": "off",
        "semi": ["error", "never"],
        "indent": ["error", "tab"],
        "no-tabs": 0,
        "eqeqeq": "off",
        "no-var": 0,
        "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
        
    }
}
